const getRandom=(n,t)=>t>n?Math.floor(Math.random()*(t+1-n))+n:"NaN",division=(n,t)=>Math.floor(n/t),version="v1.0.0",percent=(n,t)=>n/100*t,get={byid:n=>document.getElementById(n),byclass:n=>document.getElementsByClassName(n)},query={single:n=>document.querySelector(n),all:n=>document.querySelectorAll(n)},log=n=>console.log(n),root={square:n=>Math.pow(n,.5),cube:n=>Math.pow(n,1/3)},portion={integer:n=>Math.trunc(n),decimal:n=>n>-1?parseFloat("0."+(""+n).split(".")[1]):parseFloat("-0."+(""+n).split(".")[1])}
